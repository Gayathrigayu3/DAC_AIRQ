    "def calculate_si(so2):\n",
    "    si=0\n",
    "    if (so2<=40):\n",
    "     si= \"s1\"\n",
    "    if (so2>40 and so2<=80):\n",
    "     si= \"s2\"\n",
    "    if (so2>80 and so2<=380):\n",
    "     si= \"s3\"\n",
